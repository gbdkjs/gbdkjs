{
  "extends": ["eslint:recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "node": true,
    "es6": true,
    "browser": true
  },
  "globals": {
    "mergeInto": true,
    "LibraryManager": true,
    "g": true,
    "uint": true,
    "getValue": true,
    "define": true,
    "Module": true
  }
}
